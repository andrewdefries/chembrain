

paste Nucleotides.smi Nucleotides.name > NucleotidesAllWNames.smi


babel NucleotidesAllWNames.smi -O NucleotidesAllWNames.svg
babel NucleotidesAllWNames.smi -O NucleotidesAllWNames.sdf --gen2d


babel NucleotidesAllWNames.smi -O NucleotidesAllWNames_3d.svg --gen3d
babel NucleotidesAllWNames.smi -O NucleotidesAllWNames_3d.sdf --gen3d

echo "done"


