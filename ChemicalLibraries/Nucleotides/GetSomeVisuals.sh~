

paste Nucleotides.smi Nucleotides.name > NucleotidesAllWNames.smi > BabelLog


babel NucleotidesAllWNames.smi -O NucleotidesAllWNames.svg >> BabelLog
babel NucleotidesAllWNames.smi -O NucleotidesAllWNames.sdf --gen2d >> BabelLog


babel NucleotidesAllWNames.smi -O NucleotidesAllWNames_3d.svg --gen3d >> BabelLog
babel NucleotidesAllWNames.smi -O NucleotidesAllWNames_3d.sdf --gen3d >> BabelLog

echo "done"


#systematic

